{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
      "title": {
        "type": "string"
      },
      "role": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          }
        },
        "required": [
          "id"
        ]
      },
      "cities": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "id": {
              "type": "integer"
            },
            "quantity": {
              "type": "integer"
            }
          },
          "required": [
            "id",
            "quantity"
          ]
        }
      },
      "salaryRange": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          }
        },
        "required": [
          "id"
        ]
      },
      "salaryNegotiable ": {
        "type": "boolean"
      }
    },
    "required": [
      "title",
      "role",
      "cities",
      "salaryNegotiable"
    ]
  }